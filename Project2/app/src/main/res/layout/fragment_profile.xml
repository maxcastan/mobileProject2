<?xml version="1.0" encoding="utf-8"?>


<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradientback"
    tools:context=".ProfileFragment">

    <TextView
        android:id="@+id/namehere"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="29dp"
        android:layout_marginTop="168dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/emailhere"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/namehere"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="257dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/bdayhere"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/namehere"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="-2dp"
        android:layout_marginBottom="193dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/idhere"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/namehere"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="144dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/rankhere"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/namehere"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="88dp"
        android:text="TextView" />
</RelativeLayout>